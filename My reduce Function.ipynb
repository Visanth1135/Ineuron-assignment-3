    "def myreduce(function,list,start=None):\n",
    "    if start is None:\n",
    "        result=0\n",
    "    else:\n",
    "        result=start\n",
    "    for increment in list:\n",
    "        result=function(result,increment)\n",
    "    return result\n",
    "\n",
    "def add(a,b):\n",
    "    s=a+b\n",
    "    return s\n",
    "\n",
    "li=[6,4,5]\n",
    "\n",
    "product=myreduce(add,li)\n",
    "product\n"
   ]
  },
